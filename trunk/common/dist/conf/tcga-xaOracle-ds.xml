<datasources>
<local-tx-datasource>
        <jndi-name>quartzOracleDS</jndi-name>
        <connection-url>jdbc:oracle:thin:@SERVER.nci.nih.gov:PORT:SCHEMA</connection-url>
        <driver-class>oracle.jdbc.OracleDriver</driver-class>
        <user-name>USERNAME</user-name>
        <password>???</password>

		<idle-timeout-minutes>30</idle-timeout-minutes>
		<!-- sql to call when connection is created -->
		<new-connection-sql>select 0 from dual</new-connection-sql>
