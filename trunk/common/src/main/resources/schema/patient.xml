<?xml version="1.0" encoding="UTF-8"?>
<tcgaBcrDataTemplate xmlns="http://tcga-data.nci.nih.gov/tcga"
        xsi:schemaLocation="http://tcga-data.nci.nih.gov/tcga tcgaBcrDataTemplate.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <tcgaBcrData>
        <bcrFileType>clinical</bcrFileType>
        <biotabFileType>clinical_patient</biotabFileType>
        <xmlData>
            <parentNode/>
            <dataNode>
                <relativePath>/tcga_bcr/patient</relativePath>
                <alternateAttributeValue>procurement_status</alternateAttributeValue>
                <elements filterType="ignore">
                    <elementName>drugs</elementName>
                    <elementName>radiations</elementName>
                    <elementName>examinations</elementName>
                    <elementName>surgeries</elementName>
                    <elementName>follow_ups</elementName>
                    <elementName>clinical_cqcf</elementName>
                </elements>
            </dataNode>
        </xmlData>
        <biotabData>
            <keyColumns>
                <columnName>bcr_patient_barcode</columnName>
            </keyColumns>
            <dataColumnsOrder>
                <columnName>bcr_patient_barcode</columnName>
            </dataColumnsOrder>
            <diseaseSpecificData />
        </biotabData>
    </tcgaBcrData>
</tcgaBcrDataTemplate>