<?xml version="1.0" encoding="UTF-8"?>
<tcgaBcrDataTemplate xmlns="http://tcga-data.nci.nih.gov/tcga"
        xsi:schemaLocation="http://tcga-data.nci.nih.gov/tcga tcgaBcrDataTemplate.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <tcgaBcrData>
        <bcrFileType>biospecimen</bcrFileType>
        <bcrFileType>control</bcrFileType>
        <biotabFileType>biospecimen_sample</biotabFileType>
        <xmlData>
            <parentNode/>
            <dataNode>
                <relativePath>/tcga_bcr/patient/samples/sample</relativePath>
                <alternateAttributeValue>procurement_status</alternateAttributeValue>
                <elements filterType="ignore">
                    <elementName>portions</elementName>
                    <elementName>diagnostic_slides</elementName>
                </elements>
            </dataNode>
        </xmlData>
        <biotabData>
            <keyColumns>
                <columnName>bcr_sample_barcode</columnName>
            </keyColumns>
            <dataColumnsOrder>
                <columnName>bcr_sample_barcode</columnName>
            </dataColumnsOrder>
            <diseaseSpecificData/>
        </biotabData>
    </tcgaBcrData>
</tcgaBcrDataTemplate>