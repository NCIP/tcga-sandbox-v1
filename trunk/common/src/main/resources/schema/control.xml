<?xml version="1.0" encoding="UTF-8"?>
<tcgaBcrDataTemplate xmlns="http://tcga-data.nci.nih.gov/tcga"
        xsi:schemaLocation="http://tcga-data.nci.nih.gov/tcga tcgaBcrDataTemplate.xsd"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <tcgaBcrData>
        <bcrFileType>control</bcrFileType>
        <biotabFileType>control</biotabFileType>
        <xmlData>
            <parentNode/>

            <dataNode>
                <relativePath>/tcga_bcr/controls/control</relativePath>
                <alternateAttributeValue>procurement_status</alternateAttributeValue>
                <elements filterType="ignore" />
            </dataNode>
        </xmlData>
        <biotabData>
            <keyColumns>
                <columnName>bcr_aliquot_barcode</columnName>
            </keyColumns>
            <dataColumnsOrder>
                <columnName>bcr_aliquot_barcode</columnName>
                <columnName>bcr_aliquot_uuid</columnName>
            </dataColumnsOrder>
            <diseaseSpecificData/>
        </biotabData>
    </tcgaBcrData>
</tcgaBcrDataTemplate>