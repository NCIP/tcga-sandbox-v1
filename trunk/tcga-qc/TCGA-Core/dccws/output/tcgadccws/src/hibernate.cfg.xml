<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
	
		<!-- Properties for the session factory -->
		<property name="connection.datasource">java:dccCommon</property>
		<property name="show_sql">false</property>
		<property name="dialect">org.hibernate.dialect.OracleDialect</property>
		<property name="max_fetch_depth">1</property>
		<property name="bytecode.use_reflection_optimizer">false</property>
		<property name="jdbc.batch_versioned_data">true</property>
		<property name="jdbc.use_streams_for_binary">true</property>
		<property name="cache.use_query_cache">false</property>
		<property name="cache.provider_class">org.hibernate.cache.EhCacheProvider</property>

		<!-- Mapped Resources -->
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Platform.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/BiospecimenBarcode.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Datatype.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Archive.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/SampleType.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/CenterType.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Tissue.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Disease.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Visibility.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Center.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/ArchiveFile.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/BcrCenter.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/DataVisibility.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/TissueSourceSite.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/ArchiveType.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/FileInfo.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/PortionAnalyte.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/DataLevel.hbm.xml" />
		<mapping resource="gov/nih/nci/ncicb/tcga/dccws/Project.hbm.xml" />
	</session-factory>
</hibernate-configuration>

        