            <xs:simpleContent>
                <xs:restriction base="utility:clinical_res_attributes">
                    <xs:enumeration value="" />
                    <xs:enumeration value="Excisional Biopsy" />
                    <xs:enumeration value="Incisional Biopsy" />
                    <xs:enumeration value="Surgical Resection" />
