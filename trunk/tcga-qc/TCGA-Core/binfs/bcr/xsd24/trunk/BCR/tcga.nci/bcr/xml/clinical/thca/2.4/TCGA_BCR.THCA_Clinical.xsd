                <xs:element ref="shared:icd_o_3_site" />
                <xs:element ref="shared:icd_o_3_histology" />
                <xs:element ref="shared:icd_10" />
                
                <!-- Follow Up -->
                <!--xs:element ref="follow_up_case_report_form_submission_reason" />
                <xs:element ref="shared:radiation_therapy" />
                <xs:element ref="shared:additional_pharmaceutical_therapy" />
                <xs:element ref="iodine_i_131_total_administered_preparation_technique_name" />
                <xs:element ref="iodine_i_131_first_administered_dose_text" />
                <xs:element ref="iodine_i_131_subsequent_administered_dose_text" />
                <xs:element ref="iodine_i_131_total_administered_dose_text" />
