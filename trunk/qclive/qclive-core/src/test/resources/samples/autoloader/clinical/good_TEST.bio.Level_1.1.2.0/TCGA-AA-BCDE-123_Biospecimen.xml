<tcga_bcr>
    <patient>
        <samples>
            <sample>
                <bcr_sample_barcode>sample-1</bcr_sample_barcode>
                <sample_type>tumor</sample_type>
                <weight>10 mg</weight>
                <portions>
                    <portion>
                        <bcr_portion_barcode>portion-1-1</bcr_portion_barcode>
                        <portion_number>A</portion_number>
                        <analytes>
                            <analyte>
                                <bcr_analyte_barcode>analyte-1-1-1</bcr_analyte_barcode>
                                <analyte_type>DNA</analyte_type>
                                <aliquots>
                                    <aliquot>
                                        <bcr_aliquot_barcode>aliquot-1-1-1-1</bcr_aliquot_barcode>
                                        <volume>1 uL</volume>
                                    </aliquot>
                                    <aliquot>
                                        <bcr_aliquot_barcode>aliquot-1-1-1-2</bcr_aliquot_barcode>
                                        <volume>1.5 uL</volume>
                                    </aliquot>
                                </aliquots>
                                <protocols>
                                    <protocol>
                                      <protocol_name>Fred</protocol_name>
                                    </protocol>
                                </protocols>
                                <dna>
                                    <pcr_successful>YES</pcr_successful>
                                </dna>
                            </analyte>
                            <analyte>
                                <bcr_analyte_barcode>analyte-1-1-2</bcr_analyte_barcode>
                                <analyte_type>RNA</analyte_type>
                                <aliquots>
                                    <aliquot>
                                        <bcr_aliquot_barcode>aliquot-1-1-2-1</bcr_aliquot_barcode>
                                        <volume>2 uL</volume>
                                    </aliquot>
                                </aliquots>
                                <rna>
                                    <rinvalue>8.2</rinvalue>
                                </rna>
                            </analyte>
                        </analytes>
                        <slides>
                            <slide>
                                <bcr_slide_barcode>slide-1-1-1</bcr_slide_barcode>
                                <section_location>top</section_location>
                            </slide>
                        </slides>
                    </portion>
                </portions>
                <tumor_pathology>
                    <lymphatic_invasion>NO</lymphatic_invasion>
                </tumor_pathology>
            </sample>
        </samples>
    </patient>
</tcga_bcr>
